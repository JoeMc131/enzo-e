 Group {
     conserved {
         field_list = [ "density" ];
     };
     make_field_conservative {
         field_list = [ "velocity_x", "velocity_y" ];
     }
     list = [ "conserved", "make_field_conservative" ];
 }
Method {
   list += ["flux_correct"];
   flux_correct {
       enable = true;
   }
}   
  